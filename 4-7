#include <stdio.h>
#include <conio.h>
int main()
{
    int n=5;
    char ch;
    printf("Enter Any Alphabet (A-Z):- ");
    scanf("%c", &ch);
    printf("\n");
    if(ch=='A'||ch=='a') {
        for(int i=1;i<=n;i++) {
        if(i==1)      printf(" *** \n");
        else if(i==3) printf("*****\n");
        else          printf("*   *\n");
        }
    }
    else if(ch=='B'||ch=='b') {
        for(int i=1;i<=n;i++) {
            if(i==1||i==5)  printf("**** \n");
            else if(i==3)   printf("*****\n");
            else            printf("*   *\n");
        }
    }
    else if(ch=='C'||ch=='c') {
        for(int i=1;i<=n;i++) {
            if(i==1||i==5)  printf("*****\n");
            else            printf("*    \n");
        }
    }
    else if(ch=='D'||ch=='d') {
        for(int i=1;i<=n;i++) {
            if(i==1||i==5)  printf("**** \n");
            else            printf("*   *\n");
        }
    }
    else if(ch=='E'||ch=='e') {
        for(int i=1;i<=n;i++) {
            if(i==1||i==3||i==5)  printf("*****\n");
            else                  printf("*   \n");
        }
    }
    else if(ch=='F'||ch=='f') {
        for(int i=1;i<=n;i++) {
            if(i==1||i==3)  printf("*****\n");
            else            printf("*   \n");
        }
    }
    else if(ch=='G'||ch=='g') {
        for(int i=1;i<=n;i++) {
            if(i==1||i==5)  printf("*****\n");
            else if(i==2)   printf("*    \n");
            else if(i==3)   printf("*  **\n");
            else            printf("*   *\n");
        }
    }
    else if(ch=='H'||ch=='h') {
        for(int i=1;i<=n;i++) {
            if(i==3)             printf("*****\n");
            else                 printf("*   *\n");
        }
    }
    else if(ch=='I'||ch=='i') {
        for(int i=1;i<=n;i++) {
            if(i==1||i==5)  printf("*****\n");
            else            printf("  *  \n");
        }
    }
    else if(ch=='J'||ch=='j') {
        for(int i=1;i<=n;i++) {
            if(i==1)       printf("*****\n");
            else if(i==5)  printf("***  \n");
            else           printf("  *  \n");
        }
    }
    else if(ch=='K'||ch=='k') {
        for(int i=1;i<=n;i++) {
            if(i==3)             printf("*\n");
            else if(i==2||i==4)  printf("* *  \n");
            else                 printf("*   *\n");
        }
    }
    else if(ch=='L'||ch=='l') {
        for(int i=1;i<=n;i++) {
            if(i==5)  printf("*****\n");
            else      printf("*    \n");
        }
    }
    else if(ch=='M'||ch=='m') {
        for(int i=1;i<=n;i++) {
            if(i==2)       printf("**   **\n");
            else if(i==3)  printf("*  *  *\n");
            else           printf("*     *\n");
        }
    }
    else if(ch=='N'||ch=='n') {
        for(int i=1;i<=n;i++) {
            if(i==2)        printf("**  *\n");
            else if(i==3)   printf("* * *\n");
            else if(i==4)   printf("*  **\n");
            else            printf("*   *\n");
        }
    }
    else if(ch=='O'||ch=='o') {
        for(int i=1;i<=n;i++) {
            if(i==1||i==5)  printf("*****\n");
            else            printf("*   *\n");
        }
    }
    else if(ch=='P'||ch=='p') {
        for(int i=1;i<=n;i++) {
            if(i==1||i==3)  printf("*****\n");
            else if(i==2)   printf("*   *\n");
            else            printf("*    \n");
        }
    }
    else if(ch=='Q'||ch=='q') {
        for(int i=1;i<=n;i++) {
            if(i==1||i==3)  printf("*****\n");
            else      printf("*   \n");
        }
    }
    else if(ch=='R'||ch=='r') {
        for(int i=1;i<=n;i++) {
            if(i==1)  printf("******\n");
            else if(i==3)   printf("******\n");
            else if(i==4)   printf("*  *  \n");
            else            printf("*    *\n");
        }
    }
    else if(ch=='S'||ch=='s') {
        for(int i=1;i<=n;i++) {
            if(i==2)        printf("*    \n");
            else if(i==4)   printf("    *\n");
            else            printf("*****\n");
        }
    }
    else if(ch=='T'||ch=='t') {
        for(int i=1;i<=n;i++) {
            if(i==1)  printf("*******\n");
            else            printf("   *   \n");
        }
    }
    else if(ch=='U'||ch=='u') {
        for(int i=1;i<=n;i++) {
            if(i==5)  printf("******\n");
            else      printf("*    *\n");
        }
    }
    else if(ch=='V'||ch=='v') {
        for(int i=1;i<=n;i++) {
            if(i==3)        printf(" *   * \n");
            else if(i==4)   printf("  * * \n");
            else if(i==5)   printf("   *   \n");
            else            printf("*     *\n");
        }
    }
    else if(ch=='W'||ch=='u') {
        for(int i=1;i<=n;i++) {
            if(i==3)        printf("*  *  *\n");
            else if(i==4)   printf("* * * *\n");
            else            printf("*     *\n");
        }
    }
    else if(ch=='X'||ch=='x') {
        for(int i=1;i<=n;i++) {
            if(i==3)             printf("  *  \n");
            else if(i==2||i==4)  printf(" * * \n");
            else                 printf("*   *\n");
        }
    }
    else if(ch=='Y'||ch=='y') {
        for(int i=1;i<=n;i++) {
            if(i==1)        printf("*   *\n");
            else if(i==2)   printf(" * * \n");
            else            printf("  *  \n");
        }
    }
    else if(ch=='Z'||ch=='z') {
        for(int i=1;i<=n;i++) {
            if(i==2)        printf("   * \n");
            else if(i==3)   printf("  *  \n");
            else if(i==4)   printf(" *   \n");
            else            printf("*****\n");
        }
    }
    else {
        printf("Invalid Input\n");
        return 0;
    }
    printf("\n");
    return 0;
}
